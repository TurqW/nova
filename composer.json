{
    "name": "anodyne/nova",
    "type": "project",
    "description": "Nova 2 RPG Management",
    "keywords": [
        "cms",
        "anodyne",
        "nova",
        "rpg"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "fakerphp/faker": "^1.21",
        "guzzlehttp/guzzle": "^7.5",
        "illuminate/http": "^8.0",
        "swiftmailer/swiftmailer": "^6.3"
    },
    "autoload": {},
    "config": {
        "bin-dir": "nova/vendor/bin",
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "vendor-dir": "nova/vendor",
        "platform": {
            "php": "7.4"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
